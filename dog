repeat wait() until game:IsLoaded()
   game:GetService("Players").LocalPlayer.DevCameraOcclusionMode = "Invisicam"

   local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/miroeramaa/TurtleLib/main/TurtleUiLib.lua"))()
   local window = library:Window("Field of Balls")
   local vu = game:GetService("VirtualUser")
   local sword = {}
   local TweenService = game:GetService("TweenService")
   local TravelSpeed = 200000
   local x = 25
   local y = 25
   local z = 25
   local vc = game:GetService("VirtualUser")
   game:GetService("Players").LocalPlayer.Idled:Connect(function()
   vc:CaptureController()
   vc:ClickButton2(Vector2.new())
   end)
   local function sexx(sex1 : CFrame)
      local DistanceBetweenPoints = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - sex1.Position ).Magnitude
      local TimeNeeded = DistanceBetweenPoints / TravelSpeed, 3
      local Tween = TweenService:Create(
      game.Players.LocalPlayer.Character.HumanoidRootPart,
      TweenInfo.new(
      TimeNeeded,
      Enum.EasingStyle.Linear,
      Enum.EasingDirection.Out
      ),
      {
         CFrame = sex1
      }
      )
      Tween:Play()
   end
   local sword = {"Nothing"}

   for _,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
      if v:IsA("Tool") then
         if table.find(sword, v.Name) == nil then
            table.insert(sword, v.Name)
         end
      end
   end
   window:Button("READ B4 USE", function()
   loadstring(game:HttpGet('https://pastebin.com/raw/KWnZZPEh'))()
   end)
   window:Label("-------------------------", Color3.fromRGB(255, 255, 255))
   local dropdown = window:Dropdown("First Chosen Weapon", sword, function(name)
   getgenv().boobs = name
   end)
   local dropdown = window:Dropdown("Second Chosen Weapon", sword, function(name)
   getgenv().booobs = name
   end)
   local dropdown = window:Dropdown("Third Chosen Weapon", sword, function(name)
   getgenv().boooobs = name
   end)
   local dropdown = window:Dropdown("Fourth Chosen Weapon", sword, function(name)
   getgenv().booooobs = name
   end)
   local dropdown = window:Dropdown("Fifth Chosen Weapon", sword, function(name)
   getgenv().boooooobs = name
   end)
   local dropdown = window:Dropdown("Sixth Chosen Weapon", sword, function(name)
   getgenv().booooooobs = name
   end)

   window:Toggle("Auto Equip Weapon", false, function(bool)
   _G.e = bool
   while _G.e do
      pcall(function()
      game.Players[game.Players.LocalPlayer.Name].Character.Humanoid:EquipTool(
      game.Players[game.Players.LocalPlayer.Name].Backpack[boobs])
      game.Players.LocalPlayer.Backpack[booobs].Parent = game.Players.LocalPlayer.Character
      game.Players.LocalPlayer.Backpack[boooobs].Parent = game.Players.LocalPlayer.Character
      game.Players.LocalPlayer.Backpack[booooobs].Parent = game.Players.LocalPlayer.Character
      game.Players.LocalPlayer.Backpack[boooooobs].Parent = game.Players.LocalPlayer.Character
      game.Players.LocalPlayer.Backpack[booooooobs].Parent = game.Players.LocalPlayer.Character
      end)
      wait()
   end
   end)
   local balls = window:Dropdown("Farm Method", {"Everything","General"}, function(hi)
   getgenv().help = hi
   end)
   local function dn()
      spawn(function()
      while _G.y do
      pcall(function()
         if not game.Players.LocalPlayer.Character:FindFirstChild(boobs) then
            keypress(0x31)
         elseif not game.Players.LocalPlayer.Character:FindFirstChild(booobs) then
            keypress(0x31)
         elseif not game.Players.LocalPlayer.Character:FindFirstChild(boooobs) then
            keypress(0x31)
         elseif not game.Players.LocalPlayer.Character:FindFirstChild(booooobs) then
            keypress(0x31)
         elseif not game.Players.LocalPlayer.Character:FindFirstChild(boooooobs) then
            keypress(0x31)
         elseif not game.Players.LocalPlayer.Character:FindFirstChild(booooooobs) then
            keypress(0x31)
         end
         end)
         task.wait()
      end
      end)
   end
   window:Toggle("Autofarm Toggle", false, function(bool)
   _G.y = bool
   dn()
   if _G.y == false then
      wait()
      game.Players.LocalPlayer.Character.Humanoid.Health = 0
   end
   while _G.y do
      pcall(function()
      for i,v in pairs(game:GetService("Workspace").Unbreakable.Characters:GetDescendants()) do
         if v.Name == game.Players.LocalPlayer.Name then
            _G.bam = v.Parent.Name
            task.wait()
         end
      end
      if getgenv().help == "Everything" then
         if _G.bam == "Human" then
            for i,v in pairs(game.Workspace.Unbreakable.Characters.Orc:GetChildren()) do
               if not string.find(v.Name, "General") and v.IsNpc.Value == true then
                  repeat
                     pcall(function()
                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                     for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                        if v.ClassName == "Tool" then
                           v.Handle.Size = Vector3.new(x, y, z)
                        end
                     end
                     for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v:IsA("BasePart") then
                           v.CanCollide = false
                        end
                     end
                     end)
                     task.wait()
                  until v.Humanoid.Health <= 0 or _G.y == false
               end
            end
            for i,v in pairs(game.Workspace.Unbreakable.Characters.Orc:GetChildren()) do
               if string.find(v.Name, "General") and v.IsNpc.Value == true then
                  repeat
                     pcall(function()
                     game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                     game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                     for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                        if v.ClassName == "Tool" then
                           v.Handle.Size = Vector3.new(x, y, z)
                        end
                     end
                     for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                        if v:IsA("BasePart") then
                           v.CanCollide = false
                        end
                     end
                     end)
                     task.wait()
                  until v.Humanoid.Health <= 0 or _G.y == false
               end
            end
         elseif
            _G.bam == "Neutral" then
               wait()
            else
               for i,v in pairs(game.Workspace.Unbreakable.Characters.Human:GetChildren()) do
                  if not string.find(v.Name, "General") and v.IsNpc.Value == true then
                     repeat
                        pcall(function()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                        for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                           if v.ClassName == "Tool" then
                              v.Handle.Size = Vector3.new(x, y, z)
                           end
                        end
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                           if v:IsA("BasePart") then
                              v.CanCollide = false
                           end
                        end
                        end)
                        task.wait()
                     until v.Humanoid.Health <= 0 or _G.y == false
                  end
               end
               for i,v in pairs(game.Workspace.Unbreakable.Characters.Human:GetChildren()) do
                  if string.find(v.Name, "General") and v.IsNpc.Value == true then
                     repeat
                        pcall(function()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                        for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                           if v.ClassName == "Tool" then
                              v.Handle.Size = Vector3.new(x, y, z)
                           end
                        end
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                           if v:IsA("BasePart") then
                              v.CanCollide = false
                           end
                        end
                        end)
                        task.wait()
                     until v.Humanoid.Health <= 0 or _G.y == false
                  end
               end
            end
         end
         if getgenv().help == "General" then
            if _G.bam == "Human" then
               for i,v in pairs(game.Workspace.Unbreakable.Characters.Orc:GetChildren()) do
                  if string.find(v.Name, "General") and v.IsNpc.Value == true then
                     repeat
                        pcall(function()
                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                        game.Workspace.Unbreakable.Characters.Human[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                        for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                           if v.ClassName == "Tool" then
                              v.Handle.Size = Vector3.new(x, y, z)
                           end
                        end
                        for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                           if v:IsA("BasePart") then
                              v.CanCollide = false
                           end
                        end
                        end)
                        task.wait()
                     until v.Humanoid.Health <= 0 or _G.y == false
                  end
               end
            elseif
               _G.bam == "Neutral" then
                  wait()
               else
                  for i,v in pairs(game.Workspace.Unbreakable.Characters.Human:GetChildren()) do
                     if string.find(v.Name, "General") and v.IsNpc.Value == true then
                        repeat
                           pcall(function()
                           game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Torso.CFrame * CFrame.Angles(math.rad(90), 0, 0) + Vector3.new(0, -13.5, 0)
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boobs):Activate()
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booobs):Activate()
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooobs):Activate()
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooobs):Activate()
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(boooooobs):Activate()
                           game.Workspace.Unbreakable.Characters.Orc[game.Players.LocalPlayer.Name]:FindFirstChild(booooooobs):Activate()
                           for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                              if v.ClassName == "Tool" then
                                 v.Handle.Size = Vector3.new(x, y, z)
                              end
                           end
                           for i, v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
                              if v:IsA("BasePart") then
                                 v.CanCollide = false
                              end
                           end

                           end)
                           task.wait()
                        until v.Humanoid.Health <= 0 or _G.y == false
                     end
                  end
               end
            end
            end)
            task.wait()
         end
         task.wait()
         end)
         library:Keybind("RightShift")
         window:Label("RightShift to toggle UI", Color3.fromRGB(127, 143, 166))
